{
  /// The identifier (as hex-string) that zenoh runtime will use.
  /// If not set, a random UUIDv4 will be used.
  //id: "A00001",

  mode: "router",

  connect: {
    endpoints: [],
  },

  listen: {
    endpoints: [
      "tcp/0.0.0.0:7447",
      "ws/0.0.0.0:8000",
    ],
  },

  scouting: {
    multicast: {
      enabled: true,
      address: "224.0.0.224:7446",
      interface: "auto",
    },
  },

  timestamping: {
    enabled: true,
  },

  plugins: {
    /// Storage manager plugin
    storage_manager: {
      volumes: {
        memory: {
          backend: "memory",
        },
      },
      storages: {
        fendtastic: {
          key_expr: "fendtastic/**",
          volume: "memory",
        },
        murph: {
          key_expr: "murph/**",
          volume: "memory",
        },
      },
    },
  },
}
